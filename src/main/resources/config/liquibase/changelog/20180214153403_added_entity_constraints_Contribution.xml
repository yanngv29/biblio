<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Contribution.
    -->
    <changeSet id="20180214153403-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="publication_id"
                                 baseTableName="contribution"
                                 constraintName="fk_contribution_publication_id"
                                 referencedColumnNames="id"
                                 referencedTableName="publication"/>

        <addForeignKeyConstraint baseColumnNames="acte_id"
                                 baseTableName="contribution"
                                 constraintName="fk_contribution_acte_id"
                                 referencedColumnNames="id"
                                 referencedTableName="acte"/>

        <addForeignKeyConstraint baseColumnNames="ouvrage_id"
                                 baseTableName="contribution"
                                 constraintName="fk_contribution_ouvrage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ouvrage"/>

        <addForeignKeyConstraint baseColumnNames="revue_id"
                                 baseTableName="contribution"
                                 constraintName="fk_contribution_revue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="revue"/>

        <addForeignKeyConstraint baseColumnNames="memoire_id"
                                 baseTableName="contribution"
                                 constraintName="fk_contribution_memoire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="memoire"/>

    </changeSet>
</databaseChangeLog>
