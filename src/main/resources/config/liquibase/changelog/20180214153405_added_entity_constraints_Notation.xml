<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Notation.
    -->
    <changeSet id="20180214153405-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="conference_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_conference_id"
                                 referencedColumnNames="id"
                                 referencedTableName="conference"/>

        <addForeignKeyConstraint baseColumnNames="memoire_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_memoire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="memoire"/>

        <addForeignKeyConstraint baseColumnNames="ouvrage_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_ouvrage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ouvrage"/>

        <addForeignKeyConstraint baseColumnNames="publication_gouvernementale_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_publication_gouvernementale_id"
                                 referencedColumnNames="id"
                                 referencedTableName="publication_gouvernementale"/>

        <addForeignKeyConstraint baseColumnNames="rapport_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_rapport_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rapport"/>

        <addForeignKeyConstraint baseColumnNames="revue_id"
                                 baseTableName="notation"
                                 constraintName="fk_notation_revue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="revue"/>

    </changeSet>
</databaseChangeLog>
